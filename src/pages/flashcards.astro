---
import Layout from '../layouts/Layout.astro';
import { BrainCircuit } from 'lucide-vue-next';
import FlashcardsList from '../components/flashcards/FlashcardsList.vue';
export const prerender = false;
---

<Layout title="My Flashcards">
  <main class="container mx-auto px-4 py-8">
    <h1 class="mb-8 flex items-center gap-2 text-4xl font-bold text-slate-700">
      <BrainCircuit class="h-9 w-9" />
      My Flashcards
    </h1>

    <FlashcardsList client:load />
  </main>
</Layout>
